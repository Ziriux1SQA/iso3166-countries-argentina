{
  "name": "iso3166-argentina-location-db",
  "version": "1.0.0",
  "description": "Mini project for ISO 3166-2 location tables with Argentina data",
  "main": "dist/index.js",
  "author": "Marcos Wendy",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "data:download": "ts-node src/scripts/download-data.ts",
    "seed": "ts-node src/scripts/seed-locations.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/config/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/config/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/data-source.ts"
  },
  "dependencies": {
    "csv-parse": "^5.5.2",
    "reflect-metadata": "^0.1.13",
    "sql.js": "^1.9.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^1.1.0"
  }
}
